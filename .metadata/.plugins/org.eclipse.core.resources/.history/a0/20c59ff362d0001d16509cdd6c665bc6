package concepts;

public class ExceptionQues {

	public static void main(String[] args) {
		ExceptionQues obj = new ExceptionQues();
		obj.m();

	}
	
	public void m() {
		throw new RuntimeException(new Exception("This is an Exception"));
	}
	
	public void m2() {
		throw new RuntimeException(new Exception("This is an Exception"));
	}

}
