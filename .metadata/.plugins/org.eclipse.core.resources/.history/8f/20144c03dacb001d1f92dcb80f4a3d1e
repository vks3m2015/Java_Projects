import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Test {

	public static void main(String[] args) {
		
		int[] arr = {1,2,3,4,5,6};
		
		//6 5, 4, 3 2 1
		int start = 0, end = arr.length -1, temp;
		
		while(start < end) {
			temp = arr[start];
			arr[start++] = arr[end];
			arr[end--] = temp;
		}
		
		System.out.println(Arrays.toString(arr));
		
		
		
		
		
		
		
	}

}
